<UserControl
	x:Class="GetnMethods.CoreView"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:ui="using:FluentAvalonia.UI.Controls"
	xmlns:vm="using:GetnMethods.ViewModels"
	xmlns:uip="using:FluentAvalonia.UI.Controls.Primitives"
	xmlns:local="using:GetnMethods.Controls"
	x:DataType="vm:MainViewViewModel"
	x:CompileBindings="True"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">

	<Grid RowDefinitions="Auto,*">
		<ui:NavigationView AlwaysShowHeader="False"
                           PaneDisplayMode="Left"
                           Grid.RowSpan="2"
                           Name="NavView"
                           IsSettingsVisible="False"
                           IsBackEnabled="{Binding #FrameView.CanGoBack}">
			<ui:Frame Name="FrameView" />
		</ui:NavigationView>

	</Grid>
</UserControl>
